{
  "name": "n8n-nodes-magento-rest",
  "version": "0.1.0",
  "description": "An enhanced n8n community node for Magento REST API with searchCriteria support and additional features",
  "keywords": [
    "n8n-community-node-package",
    "magento",
    "magento2",
    "magento-rest",
    "ecommerce"
  ],
  "license": "MIT",
  "homepage": "https://github.com/bwilliamson55",
  "author": {
    "name": "Ben Williamson",
    "email": "bwilliamson@mail.weeumson.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Bwilliamson55/n8n-nodes-magento-rest.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/Magento/Magento.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/request-promise-native": "~1.0.21",
    "@typescript-eslint/parser": "~5.45.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.4",
    "gulp": "^4.0.2",
    "n8n-core": "^1.122.5",
    "n8n-workflow": "^2.2.1",
    "prettier": "^2.8.8",
    "typescript": "~5.3.3"
  }
}

